setwd("~/Research/Test2")
titanic3 <- read.csv("~/Research/Test2/titanic3.csv",na.strings = "")
library(dplyr)
titanic3 %>% filter(titanic3$embarked==NA)
titanic3$age[which(is.na(titanic3$age))]<-mean(titanic3$age,na.rm=TRUE)
View(titanic3)
titanic4<-titanic3 %>% mutate(Has_cab_number=ifelse(!is.na(titanic3$cabin),1,0))
View(titanic4)
str(titanic4)
setwd("~/Research/Test2")
write.csv(titanic4, file=("titanic4.csv"))
